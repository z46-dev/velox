int fib(int n) {
    if (n == 0) {
        return 0;
    }

    if (n == 1) {
        return 1;
    }

    return fib(n - 1) + fib(n - 2);
}

float funny(float x) {
    if (x < 0.5) {
        x *= 10.0;
    }

    return x;
}

int main() {
    printf(fib(10));
    printf(funny(0.0));

    return 0;
}